{
  "name": "@baidu/h5-bot-sdk",
  "version": "1.13.0",
  "description": "DuerOS H5 BOT JS SDK",
  "main": "./src/main.js",
  "scripts": {
    "test": "jest --coverage",
    "buildSingleFile": "npm run test && webpack --config webpack.sdk.js",
    "buildSingleFileTril": "npm run test && webpack --config webpack.sdktril.js",
    "buildModule": "npm run test && babel ./src -d lib",
    "build": "npm run buildSingleFile && npm run buildModule",
    "buildTril": "npm run buildSingleFileTril && npm run buildModule",
    "buildAll": "npm run buildSingleFile && npm run buildModule && npm run buildSingleFileTril"
  },
  "author": "dengxuening",
  "license": "Apache-2.0",
  "devDependencies": {
    "@comandeer/babel-plugin-banner": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^23.6.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "test": "^3.2.1"
  }
}
